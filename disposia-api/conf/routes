GET   /                       controllers.HomeController.index

POST  /api/v1/propose         controllers.HomeController.propose

->    /v1/posts               v1.post.PostRouter
GET   /api/v1/search          v1.search.SearchController.search(q: String, p: Option[Int], s: Option[Int])
GET   /api/v1/podcast/:id     v1.podcast.PodcastController.find(id: String)

# Map static resources from the /public folder to the /assets URL path
GET   /assets/*file           controllers.Assets.at(path="/public", file)

